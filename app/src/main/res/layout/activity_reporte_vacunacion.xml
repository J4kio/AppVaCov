<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReporteVacunacion">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="9dp"
        android:layout_marginRight="9dp"
        android:layout_marginBottom="38dp"
        android:gravity="center"
        android:text="Cedula Paciente: "
        app:layout_constraintBottom_toBottomOf="@+id/cedula_personal_reporte"
        app:layout_constraintEnd_toStartOf="@+id/cedula_paciente_reporte"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:gravity="center"
        android:text="Cedula Personal de Vacunacion: "
        app:layout_constraintBaseline_toBaselineOf="@+id/cedula_personal_reporte"
        app:layout_constraintEnd_toStartOf="@+id/cedula_personal_reporte"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/fecha_reporte"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="33dp"
        android:layout_marginBottom="10dp"
        android:gravity="center"
        android:text="############"
        app:layout_constraintBottom_toTopOf="@+id/sede_reporte"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/cedula_personal_reporte" />

    <TextView
        android:id="@+id/cedula_paciente_reporte"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="33dp"
        android:layout_marginBottom="9dp"
        android:gravity="center"
        android:text="00000000000000"
        app:layout_constraintBottom_toTopOf="@+id/cedula_personal_reporte"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView1"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginEnd="9dp"
        android:layout_marginRight="9dp"
        android:gravity="center"
        android:text="Fecha de Vacunacion: "
        app:layout_constraintBaseline_toBaselineOf="@+id/fecha_reporte"
        app:layout_constraintEnd_toStartOf="@+id/fecha_reporte"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button1"
        android:layout_width="146dp"
        android:layout_height="45dp"
        android:layout_marginBottom="150dp"
        android:text="Vacunar"
        app:backgroundTint="#4CAF50"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/marca_reporte" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginStart="31dp"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="95dp"
        android:layout_marginEnd="9dp"
        android:layout_marginRight="9dp"
        android:layout_marginBottom="95dp"
        android:gravity="center"
        android:text="Sede: "
        app:layout_constraintBottom_toBottomOf="@+id/marca_reporte"
        app:layout_constraintEnd_toStartOf="@+id/sede_reporte"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cedula_paciente_reporte" />

    <Spinner
        android:id="@+id/marca_reporte"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="27dp"
        android:layout_marginLeft="27dp"
        android:layout_marginEnd="27dp"
        android:layout_marginRight="27dp"
        android:layout_marginBottom="13dp"
        android:entries="@array/marcas"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/button1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lote_reporte" />

    <TextView
        android:id="@+id/sede_reporte"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginEnd="34dp"
        android:layout_marginRight="34dp"
        android:layout_marginBottom="15dp"
        android:gravity="center"
        android:text="############"
        app:layout_constraintBottom_toTopOf="@+id/lote_reporte"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/fecha_reporte" />

    <EditText
        android:id="@+id/lote_reporte"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="27dp"
        android:layout_marginLeft="27dp"
        android:layout_marginEnd="27dp"
        android:layout_marginRight="27dp"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:gravity="center"
        android:hint="@string/id_lote"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/marca_reporte"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sede_reporte" />

    <TextView
        android:id="@+id/cedula_personal_reporte"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="33dp"
        android:layout_marginBottom="14dp"
        android:gravity="center"
        android:text="00000000000000"
        app:layout_constraintBottom_toTopOf="@+id/fecha_reporte"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/cedula_paciente_reporte" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="216dp"
        android:layout_height="180dp"
        android:layout_marginTop="47dp"
        android:layout_marginBottom="39dp"
        app:layout_constraintBottom_toTopOf="@+id/cedula_paciente_reporte"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/jeringuilla" />


</androidx.constraintlayout.widget.ConstraintLayout>