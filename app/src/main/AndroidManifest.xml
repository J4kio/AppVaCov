        <activity android:name=".UsuarioVacunacion" />
        <activity android:name=".RegistroPersonalVacunacion" />
        <activity android:name=".PersonalVacunacion" />
        <activity android:name=".RegistroUsuario" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>